##############
# Xft settings
##############

Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

! transparency 
*.alpha: 0.5

! font
*.font: IBM Plex Mono:style=Regular:size=10

! special
*.foreground: 	#bbbbbb
*.background:   #1b1b1b
*.cursorColor:  #c2c2c2

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #b092cb
*.color9:       #cc6666

! green
*.color2:       #9ecbd7
*.color10:      #b5bd68

! yellow
*.color3:       #de935f
*.color11:      #af87d7

! blue
*.color4:       #727373
*.color12:      #81a2be

! magenta
*.color5:       #bcb77d
*.color13:      #b294bb
! 
! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #707880
*.color15:      #c5c8c6
